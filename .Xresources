! URXVT stuff

! transparency - true or false (default)
#if SERVERHOST==eliza
	URxvt*transparent: true
	#define bgcolor #3F3F3F
#else
	URxvt*transparent: false
	#define bgcolor [10]#3F3F3F
#endif

URxvt*depth: 32

URxvt*termName: rxvt-unicode

! shading - 0 to 99 darkens, 101 to 200 lightens.
!   Dont use with tintColor; just use a darker or lighter color instead.
!URxvt*shading: 10
!URxvt*shading: 100

! change default colors
URxvt*background: bgcolor
URxvt*foreground: #dcdccc

URxvt*intensityStyles: false

URxvt*color0:  #000000
URxvt*color1:  #CC0000
URxvt*color2:  #4E9A06
URxvt*color3:  #C4A000
URxvt*color4:  #3465A4
URxvt*color5:  #75507B
URxvt*color6:  #06989A
URxvt*color7:  #D3D7CF
URxvt*color8:  #555753
URxvt*color9:  #EF2929
URxvt*color10: #8AE234
URxvt*color11: #FCE94F
URxvt*color12: #729FCF
URxvt*color13: #AD7FA8
URxvt*color14: #34E2E2
URxvt*color15: #EEEEEC

! Xft-less:
URxvt*font: terminus-14,xft:Terminus for Powerline:style=Medium,xft:Code2000:antialias=false
URxvt*boldFont: terminus-bold-14,xft:Terminus for Powerline:style=Bold,xft:Code2000:style=Bold:antialias=false
URxvt*italicFont: -xos4-terminus-medium-o-normal--14-120-72-72-c-60-iso10646-1
URxvt*boldItalicFont: -xos4-terminus-bold-o-normal--0-0-72-72-c-0-iso10646-1

URxvt*internalBorder: 0

URxvt*perl-ext-common: default,matcher,vtwheel
URxvt*matcher.button: 1

URxvt*urgentOnBell: true
URxvt*pastableTabs: true

! scrollbar settings - disable all as scrolling is done by screen
URxvt*scrollBar: false
URxvt*saveLines: 0
URxvt*secondaryScroll: false

! keysyms to scroll within screen
URxvt*keysym.S-Prior:   \033[25S
URxvt*keysym.S-Next:    \033[25T

! Switching screen-windows
URxvt*keysym.S-Right:	\033[27;5;9~
URxvt*keysym.S-Left:	\033[27;6;9~
URxvt*keysym.C-Tab:	\033[27;5;9~


